	-webkit-box-shadow: none;
	border-radius: 0px;
	background-color: #aaa;
}

::-webkit-scrollbar-thumb {
	border-radius: 0px;
	-webkit-box-shadow: inset 1px 1px 6px #333;
	background-color: #666;
}
/* custom scroller END */
* {
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}

